{
    "extends": ["tslint:recommended", "tslint-config-standard", "tslint-react", "tslint-config-prettier"],
    "rules": {
        "jsx-no-lambda": false,
        "jsx-boolean-value": false,
        "object-literal-sort-keys": false,
        "jsx-wrap-multiline": true,
        "jsx-self-close": true,
        "jsx-space-before-trailing-slash": true,
        "jsx-alignment": true
    }
}
